{
    "manifest_version": 2,
    "name": "Paper2Slide",
    "version": "2.0.0",
    "description": "Generate HTML slides from PDF attachments using LLM (DeepSeek, OpenAI, Gemini)",
    "author": "Antigravity",
    "homepage_url": "https://github.com/antigravity/paper2slide",
    "icons": {
        "48": "chrome/content/icons/icon.png",
        "96": "chrome/content/icons/icon.png"
    },
    "applications": {
        "zotero": {
            "id": "paper2slide@antigravity.gemini",
            "update_url": "https://raw.githubusercontent.com/antigravity/paper2slide/main/update.json",
            "strict_min_version": "6.999",
            "strict_max_version": "7.0.*"
        }
    }
}